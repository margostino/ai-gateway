{
  "name": "ai-gateway",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=dev nodemon src/index.ts",
    "build": "yarn clean:build && tsc && tsc-alias",
    "start": "NODE_OPTIONS=--max_old_space_size=8192 NODE_ENV=production node dist/index.js",
    "deploy:stag": "./scripts/deploy.sh stag",
    "deploy:prod": "./scripts/deploy.sh prod",
    "tag-prod-release": "./scripts/tag-prod-release.sh",
    "check:ts": "tsc --noEmit",
    "check:lint": "eslint ./src --ext .js,.ts",
    "check:prettier": "prettier -u --check .",
    "check:prettier:fix": "prettier -u --write .",
    "check:spelling": "cspell '**'",
    "check:all": "concurrently --kill-others-on-fail yarn:check:ts yarn:check:lint yarn:check:prettier",
    "check:all:staged": "lint-staged",
    "clean:build": "rimraf dist .tsconfig.tsbuildinfo",
    "fix:check:all": "yarn check:prettier:fix && yarn check:all",
    "prepare": "husky install",
    "test": "OPENAI_API_KEY=FAKE jest --coverage --no-cache --detectOpenHandles --forceExit",
    "test:watch": "jest --watch"
  },
  "engines": {
    "node": ">=18",
    "npm": "please-use-yarn"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "helmet": "^7.1.0",
    "hpagent": "^1.2.0",
    "http-status-codes": "^2.3.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "pino": "^8.16.2",
    "prom-client": "^15.0.0",
    "ssl-root-cas": "^1.3.1",
    "stream": "^0.0.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/helmet": "^4.0.0",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^29.5.10",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "axios-mock-adapter": "^1.22.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "pino-pretty": "^10.2.3",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2"
  }
}
